<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:autoTransition="animateToEnd">
        <KeyFrameSet>
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/backgroundLayout"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="150dp"
            android:layout_height="150dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="10"
            android:scaleY="10"
            motion:layout_constraintVertical_bias="0.512"
            motion:layout_constraintHorizontal_bias="0.436" />
        <Constraint
            android:id="@+id/blueTwitter"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="173dp"
            android:layout_height="188dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="0"
            android:scaleY="0"
            android:alpha="0"
            motion:layout_constraintVertical_bias="0.548"
            motion:layout_constraintHorizontal_bias="0.428" />
        <Constraint
            android:id="@+id/whiteTwitter"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="177dp"
            android:layout_height="180dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="1"
            android:scaleY="1"
            motion:layout_constraintHorizontal_bias="0.418"
            motion:layout_constraintVertical_bias="0.54" />
        <Constraint
            motion:layout_constraintVertical_bias="0.966"
            android:layout_height="250dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.496"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="304dp"
            android:id="@+id/whiteTwitterr"
            motion:layout_editor_absoluteY="465dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/backgroundLayout"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="150dp"
            android:layout_height="150dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintVertical_bias="0.108"
            motion:layout_constraintHorizontal_bias="0.8" />
        <Constraint
            android:id="@+id/blueTwitter"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="206dp"
            android:layout_height="236dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="1"
            android:scaleY="1"
            android:alpha="1"
            motion:transitionEasing="cubic(1,0,1,0)"
            motion:layout_constraintVertical_bias="0.032"
            motion:layout_constraintHorizontal_bias="0.873" />
        <Constraint
            android:id="@+id/whiteTwitter"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="172dp"
            android:layout_height="202dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintVertical_bias="0.066"
            motion:layout_constraintHorizontal_bias="0.815" />
        <Constraint
            motion:layout_constraintVertical_bias="0.97"
            android:layout_height="187dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="308dp"
            android:id="@+id/whiteTwitterr" />
    </ConstraintSet>
</MotionScene>